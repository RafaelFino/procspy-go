// Path: internal/procspy
func CreateUser(name string, key string) error {
	log.Printf("[procspy-cli] Creating user: %s", name)
	return u.storage.CreateUser(name, key)
}

func ApproveUser(name string) error {
	log.Printf("[procspy-cli] Approving user: %s", name)
	return u.storage.ApproveUser(name)
}
