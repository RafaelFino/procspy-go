{
    "db_path": "data",
    "log_path": "logs",  
    "interval": 60,
    "config_url": "https://rgt-tools.duckdns/config.json",
    "load_from_url": false,
    "targets": [
        {
            "name": "chrome",
            "limit": 7200,
            "pattern": "*chrome*",
            "kill": false
        },
        {
            "name": "firefox",
            "limit": 7200,
            "pattern": "*firefox*",
            "kill": false
        },
        {
            "name": "steam",
            "limit": 7200,
            "pattern": "*steam*",
            "kill": false
        }
    ]
}